<UserControl x:Class="GamePlugin.Playground"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:entity="clr-namespace:GamePlugin"
    mc:Ignorable="d"
    d:DesignHeight="480" d:DesignWidth="720">
    <Canvas x:Name="PlaygroundCanvas" Width="720" Height="480">
        <Canvas.Background>
                <ImageBrush Stretch="Fill" ImageSource="image/playground.png"/>
        </Canvas.Background>
    </Canvas>
</UserControl>
